{
  "@template": {
    "details": {
      "iaid": "{$.identifier.__iaid}",
      "catalogueContext": "{$.template.details.catalogueContext.value}",
      "level": "{$.level.value}",
      "legalStatus": "{$.legal.status}",
      "description": "{$.description[0].value}",
      "primaryIdentifier": "{$.@admin.id}",
      "summaryTitle": "{$.summary.title}",
      "type": "{$.@datatype.base}",
      "dateCreated": {
        "#type": "date",
        "value": "$.origination.date.value",
        "format": "yyyy"
      },
      "closureStatus": {
        "#type": "scope",
        "scope": "$.availability",
        "value": {
          "closureCode": "$.closure.code",
          "closureStatus": "$.closure.label.value",
          "closureStats": {
            "#type": "scope",
            "scope": "$.closure",
            "value": {
              "#type": "scope",
              "scope": "$.label",
              "value": "$.value"
            }
          }
        }
      },
      "heldBy": "{$.repository.name.value}",
      "referenceNumber": "{$.identifier.__reference_number}",
      "formerDepartmentReference": "{$.identifier.__former_reference_department}",
      "heldById": "{$.repository.@admin.id}",
      "deliveryOption": "{$.availability.delivery.condition.value}"
    }
  }
}